    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) dev_vme_config_model_view",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/dev_vme_config_model_view",
            "args": ["/home/florian/Documents/mvme-workspaces-on-hdd/gsi-nick_kurz/all_modules/all_modules.vme"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) mvme tracing",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/mvme",
            "args": ["--trace-mvlc"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }

    ]
}
